{
  "autostart":1,
  "timeout":250,
  "alignment":"top+hclient",
  "width":75,
  "height":75,
  "maximized":0,
  "clip":1,
  "badge":1,
  "show-help":1,
  "hk":{
    "appear":"ctrl+0xC0",
    "full-screen":"alt+enter",
    "split":"ctrl+S",
    "expand":"ctrl+W",
    "rotate":"ctrl+A",
    "next":"ctrl+tab",
    "prev":"ctrl+shift+tab",
    "close":"ctrl+f4",
    "ctrl-break":"ctrl+break",
    "ctrl-c":"ctrl+C"
  },
  "mode":{
    "ad":{
      "background-color":"F00F0F0F",
      "selection-color":"00FF00",
      "languare-from":"",
      "languare-to":""
    },
    "console":{
      "background-color":"400F0F0F",
      "order-active":"FFFFFF",
      "order-inactive":"404040",
      "fonts":{
        "main":{
          "type-face":"Consolas",
          "size":16,
          "color":"FFFFFF"
        },
        "sub":{
          "type-face":"Consolas",                                                                                              
          "size":12,
          "color":"008000"
        }
      }
    }
  }
}
